@article{francino2016,
  title = {Antibiotics and the {{Human Gut Microbiome}}: {{Dysbioses}} and {{Accumulation}} of {{Resistances}}},
  shorttitle = {Antibiotics and the {{Human Gut Microbiome}}},
  author = {Francino, M. P.},
