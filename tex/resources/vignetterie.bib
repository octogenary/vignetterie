@article{hover2018,
  title = {Culture-independent discovery of the malacidins as calcium-dependent antibiotics with activity against multidrug-resistant {{Gram-positive}} pathogens},
  author = {Hover, Bradley M. and Kim, Seong-Hwan and Katz, Micah and Charlop-Powers, Zachary and Owen, Jeremy G. and Ternei, Melinda A. and Maniko, Jeffrey and Estrela, Andreia and Molina, Henrik and Park, Steven and Perlin, David S. and Brady, Sean F.},
  date = {2018-04},
  journaltitle = {Nature microbiology},
